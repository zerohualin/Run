<module name="hunter">

	<enum name="CardType" comment="道具品质">
		<var name="Action" alias="行动" comment="白"/>
		<var name="Response" alias="响应" comment="白"/>
	</enum>

	<bean name="CardConfig" comment="卡牌">
		<var name="id" type="int" comment="卡牌id"/>
		<var name="name" type="string"/>
		<var name="cost" type="int"/>
		<var name="type" type="CardType"/>
		<var name="desc" type="string"/>
	</bean>

	<table name="TbCardConfig" value="CardConfig" input="Hunter/card.xlsx" comment="道具表"/>
</module>